package module2;

public class lineincrementpyramid {

	public static void main(String[] args) {
		for (int i = 1; i <= 5; i++) {

            for (int j = i; j < i+1; j++) {
            	
            	System.out.print(j+"\t");	
             
            }
            System.out.println();
        }
	}

}
