package module2;

public class lineincrementpyramid {

	public static void main(String[] args) {
		for (int i = 1; i <= 5; i++) {

            for (int j = i; j < 5; j++) {
            	
            	System.out.print(i+j+"\t");	
             
            }
            System.out.println();
        }
	}

}
